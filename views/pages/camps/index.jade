extends ../../includes/page
block content
    .camps
        section.jumbotron.text-center
            .container
                h1.jumbotron-heading=t('camps.dash.title')
                p.lead.text-muted=t('camps.dash.desc')
                p
                    a.Btn.Btn--default.reveal_create_camp_btn(href='#', role='button')=t('camps.new.btn_create')

        section.jumbotron.card.choose_name.hidden
            .center-block
                h2=t('camps.new.header_title')
                h4=t('camps.new.name_choosing_title')

                .form-group.pos-r
                    label=t('camps.new.name_en')
                    input(type="text", class="form-control", name="camp_name_en", autofocus="true", required)
                    span.indicator
                        span.glyphicon
                .form-group
                    a.Btn.Btn--green.hidden(id="check_camp_name", role='button')=t('camps.btn_continue')
        section.dash-panel
            .row
                .col-md-4
                    .block-center
                        p
                            img.img-responsive(src="data:image/svg+xml;utf8;base64,")
                        a.Btn.Btn--transparent(href='/#{language}/camps-stats', role='button')=t('camps.dash.stats.title')
      
                .col-md-4
                    .block-center
                        p
                            img.img-responsive(src="data:image/svg+xml;utf8;base64,")
                        a.Btn.Btn--transparent(href='#', role='button')=t('camps.dash.members.title')

                .col-md-4
                    .block-center
                        p
                            img.img-responsive(src="data:image/svg+xml;utf8;base64,")
                        a.Btn.Btn--transparent(href='#', role='button')=t('camps.dash.docs.title')
